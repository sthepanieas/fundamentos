#include <iostream> 
#include "math.h"
using namespace std;

int main()
{
	int n;
	cout<<"ingrese n: ";
  cin>>n;

	bool k=true;
	for(int i=2;i<n;i++)
	{
		if(n%i==0)
		{ 
    k=false; 
    break;
    }
	}
	
	if(k==true)
	{
  cout<<"Es primo"<<endl;
  }
	else
  {
  cout<<"NO es primo"<<endl;
  }
return 0;

}
