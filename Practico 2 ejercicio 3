
#include "stdafx.h"
#include <iostream>
#include "conio.h"
#include "math.h"
using namespace std;
int main()
{

int n=0, suma=0;

	cout<<"Ingrese cuanto valores quiere calcular: " <<endl;
	cin>>n;

for(int i=1;i<=n;i++){
	suma = suma +powf(i,2);
	}

	cout<<"La suma de cuadrados hasta: "<< n << "es :" << suma <<endl;

	system("pause");

	return 0;
}





